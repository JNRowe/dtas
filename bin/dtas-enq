require 'dtas/unix_client'
c = DTAS::UNIXClient.new

ARGV.each do |path|
  # Unix paths are encoding agnostic
  path = File.expand_path(path.b)
  res = c.req_ok(%W(enq #{path}))
  puts "#{path} #{res}"
end
